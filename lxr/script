	var serverip="http://"+location.host;
	var graphurl=serverip+"/lxr/call";
	var listurl=serverip+"/lxr/watchlist";
	var codeurl="http://124.16.141.171:81/lxr/source/";
	var noteurl="http://124.16.141.171:81/mediawiki/index.php/";
	
	var urls = new Array(graphurl, listurl, codeurl, noteurl);
	
	function seturls() {
		urls = new Array(graphurl, listurl, codeurl, noteurl);
		return urls;
	}

	function geturls(){
		return urls;
	}


	function cblu(){
		var gblu=D.getElementById(&quot;layer2&quot;)
		if(bluv==&quot;visible&quot;){
			gblu.setAttribute(&quot;visibility&quot;,&quot;hidden&quot;)
			bluv=&quot;hidden&quot;}
		else{
			gblu.setAttribute(&quot;visibility&quot;,&quot;visible&quot;)
			bluv=&quot;visible&quot;}
	}

	function cr(){
