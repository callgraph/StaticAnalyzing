	function cz(){
		var	changezb=D.getElementById(&quot;menu0&quot;);
		changebrob=D.getElementById(&quot;menu1&quot;);
		changerb=D.getElementById(&quot;menu2&quot;);
		changegb=D.getElementById(&quot;menu3&quot;);
		changeblab=D.getElementById(&quot;menu4&quot;);
		changeblub=D.getElementById(&quot;menu5&quot;);
		changechoc=D.getElementById(&quot;menu6&quot;);
		changemoub=D.getElementById(&quot;rect3994&quot;);
		changezb.setAttribute(&quot;visibility&quot;,&quot;visible&quot;)
	
		if(b==&quot;visible&quot;){
			changeblub.setAttribute(&quot;visibility&quot;,&quot;hidden&quot;)
			changeblab.setAttribute(&quot;visibility&quot;,&quot;hidden&quot;)
			changegb.setAttribute(&quot;visibility&quot;,&quot;hidden&quot;)
			changerb.setAttribute(&quot;visibility&quot;,&quot;hidden&quot;)
			changebrob.setAttribute(&quot;visibility&quot;,&quot;hidden&quot;)
			changechoc.setAttribute(&quot;visibility&quot;,&quot;hidden&quot;)
			changemoub.setAttribute(&quot;visibility&quot;,&quot;hidden&quot;)
			b=&quot;hidden&quot;
		}
		else{
			changeblub.setAttribute(&quot;visibility&quot;,&quot;visible&quot;)
			changeblab.setAttribute(&quot;visibility&quot;,&quot;visible&quot;)
			changegb.setAttribute(&quot;visibility&quot;,&quot;visible&quot;)
			changerb.setAttribute(&quot;visibility&quot;,&quot;visible&quot;)
